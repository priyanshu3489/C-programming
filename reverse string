// reverse string without strrev
#include <stdio.h>
#include<string.h>
int main()
{
    char name1[30],name2[30];
    int len1,i,j;
    printf("Enter 1st strin:");
    gets(name1);
    len1=strlen(name1);
    for(i=len1-1,j=0;i>=0;i--,j++)
    {
        name2[j]=name1[i];
    }
    name2[j]='\0';
    printf("\nThe reverse string is %s",name2);
    return 0;
}
